{   "phase": {
    "trainAll_2frames": 15,
    "trainAll_fullgop": 18,
    "trainAll_fullgop_ME": 22
    },

"trainAll_2frames": {
    "batch_size": 12, 
    "lr": 1e-4,  
    "strategy": {"stage": "2frames", "random": 1}, 
    "mode": "residual", 
    "frozen_modules": ["ME"],         
    "loss_on": {"R": "rate", "D": "distortion"}
    },

"trainAll_fullgop": {
    "batch_size": 3, 
    "lr": 1e-4, 
    "strategy": {"stage": "fullgop", "max_num_Pframe": 5}, 
    "frozen_modules": ["ME"],
    "loss_on": {"R": "rate", "D": "distortion"}
    },

"trainAll_fullgop_ME": {
    "batch_size": 3, 
    "lr": 1e-5, 
    "strategy": {"stage": "fullgop", "max_num_Pframe": 5}, 
    "frozen_modules": [],
    "loss_on": {"R": "rate", "D": "distortion"}
    }
}